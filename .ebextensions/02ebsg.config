Resources:
  AWSEBSecurityGroup:
    Type: "AWS::EC2::SecurityGroup"
    Properties:
      GroupDescription: "Security group to allow HTTPS for all, SSH for office"
      SecurityGroupIngress:
        - {SourceSecurityGroupOwnerId : {"Fn::GetAtt" : ["AWSEBLoadBalancer", "SourceSecurityGroup.OwnerAlias"]},
           SourceSecurityGroupName: {"Fn::GetAtt" : ["AWSEBLoadBalancer", "SourceSecurityGroup.GroupName"]},
           IpProtocol: "tcp",
           FromPort: "80",
           ToPort: "80"}
        - {CidrIp: "205.251.233.32/28", IpProtocol: "tcp", FromPort: "22", ToPort: "22"}
        - {CidrIp: "205.251.233.48/29", IpProtocol: "tcp", FromPort: "22", ToPort: "22"}
        - {CidrIp: "205.251.233.160/28", IpProtocol: "tcp", FromPort: "22", ToPort: "22"}
        - {CidrIp: "205.251.233.176/29", IpProtocol: "tcp", FromPort: "22", ToPort: "22"}
        - {CidrIp: "205.251.234.32/28", IpProtocol: "tcp", FromPort: "22", ToPort: "22"}
